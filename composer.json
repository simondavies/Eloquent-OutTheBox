{
    "name": "simondavies/eloquent-outthebox",
    "description": "A simple example of using Laravel's Eloquent outside of the Laravel framework, to utilise its awesome ORM power... and beyond!",
    "keywords": ["orm", "laravel", "eloquent","php"],
    "license": "MIT",
    "type": "project",
    "version": "0.1",
    "authors": [
        {
            "name": "Simon Davies",
            "email": "simondavies@live.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "^5.4",
        "vlucas/phpdotenv": "^2.4",
        "phpunit/phpunit": "^6.0",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload":{
        "classmap": [
            "app/database/"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
